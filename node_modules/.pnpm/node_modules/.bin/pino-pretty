#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hgunduzoglu/instagram-social-verify/node_modules/.pnpm/pino-pretty@13.0.0/node_modules/pino-pretty/node_modules:/home/hgunduzoglu/instagram-social-verify/node_modules/.pnpm/pino-pretty@13.0.0/node_modules:/home/hgunduzoglu/instagram-social-verify/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/hgunduzoglu/instagram-social-verify/node_modules/.pnpm/pino-pretty@13.0.0/node_modules/pino-pretty/node_modules:/home/hgunduzoglu/instagram-social-verify/node_modules/.pnpm/pino-pretty@13.0.0/node_modules:/home/hgunduzoglu/instagram-social-verify/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pino-pretty/bin.js" "$@"
else
  exec node  "$basedir/../pino-pretty/bin.js" "$@"
fi
