#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hgunduzoglu/instagram-social-verify/node_modules/.pnpm/@noir-lang+noir_codegen@1.0.0-beta.3/node_modules/@noir-lang/noir_codegen/lib/node_modules:/home/hgunduzoglu/instagram-social-verify/node_modules/.pnpm/@noir-lang+noir_codegen@1.0.0-beta.3/node_modules/@noir-lang/noir_codegen/node_modules:/home/hgunduzoglu/instagram-social-verify/node_modules/.pnpm/@noir-lang+noir_codegen@1.0.0-beta.3/node_modules/@noir-lang/node_modules:/home/hgunduzoglu/instagram-social-verify/node_modules/.pnpm/@noir-lang+noir_codegen@1.0.0-beta.3/node_modules:/home/hgunduzoglu/instagram-social-verify/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/hgunduzoglu/instagram-social-verify/node_modules/.pnpm/@noir-lang+noir_codegen@1.0.0-beta.3/node_modules/@noir-lang/noir_codegen/lib/node_modules:/home/hgunduzoglu/instagram-social-verify/node_modules/.pnpm/@noir-lang+noir_codegen@1.0.0-beta.3/node_modules/@noir-lang/noir_codegen/node_modules:/home/hgunduzoglu/instagram-social-verify/node_modules/.pnpm/@noir-lang+noir_codegen@1.0.0-beta.3/node_modules/@noir-lang/node_modules:/home/hgunduzoglu/instagram-social-verify/node_modules/.pnpm/@noir-lang+noir_codegen@1.0.0-beta.3/node_modules:/home/hgunduzoglu/instagram-social-verify/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@noir-lang+noir_codegen@1.0.0-beta.3/node_modules/@noir-lang/noir_codegen/lib/main.js" "$@"
else
  exec node  "$basedir/../.pnpm/@noir-lang+noir_codegen@1.0.0-beta.3/node_modules/@noir-lang/noir_codegen/lib/main.js" "$@"
fi
